{
  "expo": {
    "name": "PrimeiroApp",
    "slug": "ufpr-PrimeiroApp",
    "backgroundColor": "#007bff",
    "primaryColor": "#007bff",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#007bff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aula.primeiroApp"
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#00000000",
      "translucent": true
    },
    "notification": {
      "icon": "./assets/notificacao.png"
    },
    "android": {
      "package": "com.aula.primeiroApp"
    }
  }
}
